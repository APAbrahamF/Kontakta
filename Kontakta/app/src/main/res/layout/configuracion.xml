<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"


    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/coordinatorLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuConfiguracion">

    <EditText
        android:id="@+id/editTextTextPersonName3"
        android:layout_width="227dp"
        android:layout_height="77dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Buscar..."
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="77dp"
        android:layout_height="77dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/kontakta_logo" />

    <ImageButton
        android:id="@+id/imageButton14"
        android:layout_width="77dp"
        android:layout_height="77dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@android:drawable/ic_menu_search" />

    <View
        android:id="@+id/view4"
        android:layout_width="412dp"
        android:layout_height="64dp"
        android:layout_weight="1"
        android:background="#6AA7D9"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="207dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="362dp" />

    <ImageButton
        android:id="@+id/confRecomButt"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:background="#6AA7D9"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@android:drawable/ic_menu_compass" />

    <ImageButton
        android:id="@+id/confInicioButt"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:background="#6AA7D9"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@android:drawable/ic_media_play" />

    <ImageButton
        android:id="@+id/imageButton17"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:background="#6AA7D9"
        android:tint="#FF6E40"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.962"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@android:drawable/ic_menu_manage" />

    <GridLayout
        android:id="@+id/GridLayout1"
        android:layout_width="wrap_content"
        android:layout_height="570dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:columnCount="2"
        android:orientation="vertical"
        android:rowCount="5"
        app:layout_constraintBottom_toTopOf="@+id/view4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName3"
        app:layout_constraintVertical_bias="1.0">

        <Button
            android:id="@+id/buttonCamContra"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_row="0"
            android:layout_column="0"
            android:layout_gravity="left|top"
            android:text="Cambiar contraseÃ±a"
            android:drawableTop="@drawable/password_icono"
            android:layout_marginBottom="20dp"
            android:textColor="#fff"
            android:background="@drawable/orange_background"/>

        <Button
            android:id="@+id/buttonCServ"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_row="0"
            android:layout_column="1"
            android:layout_gravity="right|top"
            android:text="modificar prestador"
            android:drawableTop="@drawable/cambiouser_icono"

            android:textColor="#fff"
            android:background="@drawable/orange_background"/>

        <Button
            android:id="@+id/buttonCUser"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_row="1"
            android:layout_column="0"
            android:layout_gravity="left|top"
            android:text="modificar usuario"
            android:drawableTop="@drawable/cambiouser_icono"
            android:layout_marginBottom="20dp"
            android:textColor="#fff"
            android:background="@drawable/orange_background"/>

        <Button
            android:id="@+id/buttonHistorial"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_row="1"
            android:layout_column="1"
            android:layout_gravity="right|top"
            android:text="ver historial"
            android:drawableTop="@drawable/historial_icono"
            android:textColor="#fff"
            android:background="@drawable/orange_background"/>

        <Button
            android:id="@+id/buttonBorrarServ"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_row="2"
            android:layout_column="0"
            android:layout_gravity="right|top"
            android:text="borrar servicio"
            android:drawableTop="@drawable/borrar_icono"
            android:layout_marginBottom="20dp"
            android:textColor="#fff"
            android:background="@drawable/orange_background"/>

        <Button
            android:id="@+id/buttonBorrarUser"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_row="2"
            android:layout_column="1"
            android:layout_gravity="left|top"
            android:text="borrar cuenta"
            android:drawableTop="@drawable/borrar_icono"
            android:textColor="#fff"
            android:background="@drawable/orange_background"/>

        <Button
            android:id="@+id/buttonRegistroNuevo"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_row="3"
            android:layout_column="0"
            android:layout_gravity="right|top"
            android:text="registrar servicio"
            android:drawableTop="@drawable/agregar_icono"
            android:layout_marginBottom="20dp"
            android:textColor="#fff"
            android:background="@drawable/orange_background"/>

        <Button
            android:id="@+id/buttonVerReviews"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_row="3"
            android:layout_column="1"
            android:layout_gravity="left|top"
            android:text="ver reviews"
            android:drawableTop="@drawable/review_icono"
            android:textColor="#fff"
            android:background="@drawable/orange_background"/>

        <Button
            android:id="@+id/testVerU"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_row="4"
            android:layout_column="0"
            android:layout_gravity="right|top"
            android:text="Test visualizar perfil Usuario"
            android:textColor="#fff"
            android:background="@drawable/orange_background"/>

        <Button
            android:id="@+id/testVerP"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_row="4"
            android:layout_column="1"
            android:layout_gravity="left|top"
            android:text="Test  perfil Prestador"
            android:textColor="#fff"
            android:background="@drawable/orange_background"/>
    </GridLayout>
</androidx.constraintlayout.widget.ConstraintLayout>